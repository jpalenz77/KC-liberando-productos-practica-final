[run]
source = src
branch = True
relative_files = True

[paths]
source =
    src/
    /mnt/c/tests/KC-liberando-productos-practica-final/src/

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    pass
    raise ImportError
precision = 2
fail_under = 70

[html]
directory = coverage_html
title = Coverage Report